<svg width="16" height="16" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <circle cx="8" cy="8" r="7" fill="#4F46E5"/>
  
  <!-- Camera body -->
  <rect x="3" y="5" width="10" height="7" rx="1" fill="#F8FAFC" stroke="#475569" stroke-width="0.3"/>
  
  <!-- Camera lens -->
  <circle cx="8" cy="8.5" r="2" fill="#1E293B"/>
  <circle cx="8" cy="8.5" r="1.2" fill="#334155"/>
  
  <!-- Flash -->
  <rect x="11" y="6" width="1" height="0.8" rx="0.2" fill="#EF4444"/>
  
  <!-- Barcode lines (very simplified) -->
  <g transform="translate(4, 2)">
    <rect x="0" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="0.5" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="1.2" y="0" width="0.5" height="1.5" fill="#FFFFFF"/>
    <rect x="2" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="2.5" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="3.2" y="0" width="0.5" height="1.5" fill="#FFFFFF"/>
    <rect x="4" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="4.7" y="0" width="0.5" height="1.5" fill="#FFFFFF"/>
    <rect x="5.5" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="6" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
    <rect x="6.7" y="0" width="0.5" height="1.5" fill="#FFFFFF"/>
    <rect x="7.5" y="0" width="0.3" height="1.5" fill="#FFFFFF"/>
  </g>
  
  <!-- Photo indicator -->
  <rect x="2" y="13" width="2" height="1.5" rx="0.2" fill="#F1F5F9" stroke="#94A3B8" stroke-width="0.1"/>
  <circle cx="3" cy="13.7" r="0.4" fill="#10B981"/>
  
  <!-- SN label indicator -->
  <rect x="12" y="13" width="2" height="1" rx="0.2" fill="#F8FAFC" stroke="#94A3B8" stroke-width="0.1"/>
  <rect x="12.2" y="13.3" width="1" height="0.1" fill="#475569"/>
  
  <!-- Scanning line -->
  <rect x="6" y="8.5" width="4" height="0.1" fill="#10B981" opacity="0.8"/>
</svg>